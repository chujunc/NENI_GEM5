#!/bin/sh

# File to run the ferret benchmark

cd /parsec/install/bin
/sbin/m5 switchcpu
/sbin/m5 dumpstats
/sbin/m5 resetstats
./ferret /parsec/install/inputs/ferret/corelm lsh /parsec/install/inputs/ferret/queriesm 10 20 16 /parsec/install/inputs/ferret/output.txt
echo "Done :D"
/sbin/m5 exit
/sbin/m5 exit
